{
    "name": "kriss/laravel-log-reader",
    "description": "This is my package laravel-log-reader",
    "keywords": [
        "kriss",
        "laravel-log-reader"
    ],
    "homepage": "https://github.com/krissss/laravel-log-reader",
    "license": "MIT",
    "authors": [
        {
            "name": "kriss",
            "email": "462679766@qq.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1|^8",
        "kriss/log-reader": "^1.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "illuminate/support": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "Kriss\\LaravelLogReader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kriss\\LaravelLogReader\\Tests\\": "tests"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Kriss\\LaravelLogReader\\LogReaderServiceProvider"
            ]
        }
    }
}
