{
    "name": "kriss/laravel-log-reader",
    "description": "This is my package laravel-log-reader",
    "keywords": [
        "kriss",
        "laravel-log-reader"
    ],
    "homepage": "https://github.com/krissss/laravel-log-reader",
    "license": "MIT",
    "authors": [
        {
            "name": "kriss",
            "email": "462679766@qq.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Kriss\\LaravelLogReader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kriss\\LaravelLogReader\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
